package com.spring.dct;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class HomeController {

	@GetMapping("/")
	public String index() {
		System.out.println("메인 화면 - index");
		return "index";
	}
	@GetMapping("/carts")
	public String carts() {
		System.out.println("메인 화면 - index");
		return "carts";
	}
}
